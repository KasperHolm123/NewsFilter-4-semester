<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NewsFilter_4_semester.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:NewsFilter_4_semester"
    xmlns:views="clr-namespace:NewsFilter_4_semester.Pages">

    <FlyoutItem Title="Feed">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}"/>
        </Tab>
        
    </FlyoutItem>
    <FlyoutItem Title="Filters">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate views:FilterPage}"/>
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="Settings">
        <Tab>
            <ShellContent ContentTemplate="{DataTemplate views:SettingsPage}"/>
        </Tab>
    </FlyoutItem>
</Shell>
